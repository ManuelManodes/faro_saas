{
  "buildCommand": "npm run build -w client",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.0.0",
      "includeFiles": "server/**"
    }
  }
}